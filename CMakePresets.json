{
  "version": 3,
  "configurePresets": [
    {
      "name": "ucrt64",
      "displayName": "UCRT64 GCC + Ninja",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "C:/msys64/ucrt64/bin/gcc.exe",
        "CMAKE_CXX_COMPILER": "C:/msys64/ucrt64/bin/g++.exe"
      }
    },
    {
      "name": "ucrt64-release",
      "inherits": "ucrt64",
      "cacheVariables": {
      "CMAKE_BUILD_TYPE": "Release"
      }
    }
  ]
}
